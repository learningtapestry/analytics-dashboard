{
  "env": {
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "globals": {
    "require": false,
    "process": false,
    "__dirname": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [
      "error", {
        "code": 90
      }
    ],
    "no-console": [
      "warn", {
        "allow": ["error"]
      }
    ],
    "no-unused-vars": ["warn"]
  }
}

